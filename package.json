{
  "name": "clawwallet",
  "version": "0.1.0",
  "description": "One-click Solana wallets for AI agents",
  "private": true,
  "workspaces": [
    "sdk",
    "api"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "build:sdk": "npm run build -w sdk",
    "build:api": "npm run build -w api",
    "start:api": "npm run start -w api",
    "dev:api": "npm run dev -w api"
  },
  "keywords": [
    "solana",
    "wallet",
    "ai",
    "agents"
  ],
  "author": "ClawWallet",
  "license": "MIT",
  "devDependencies": {
    "@solana/web3.js": "^1.98.4",
    "borsh": "^2.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
